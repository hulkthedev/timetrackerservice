{
  "name": "hulk/timetrackerservice",
  "license": "proprietary",
  "type": "project",
  "description": "REST based TimeTracker Service",
  "version": "1.0.0",
  "autoload": {
    "psr-4": {
      "Hulk\\TimeTracker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hulk\\TimeTracker\\": "tests/unit/"
    }
  },
  "config": {
    "http-basic": {},
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "apcu-autoloader": true
  },
  "require": {
    "php": ">=7.3",
    "ext-apcu": "*",
    "ext-curl": "*",
    "ext-igbinary": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-zlib": "*",
    "doctrine/doctrine-bundle": "^2",
    "jms/serializer-bundle": "^3.5",
    "symfony/dependency-injection": "^5",
    "symfony/flex": "^1",
    "symfony/framework-bundle": "^5",
    "symfony/intl": "^5",
    "symfony/monolog-bundle": "^3",
    "symfony/validator": "^5",
    "symfony/yaml": "^5"
  },
  "require-dev": {
    "phpunit/phpunit": "~8"
  },
  "repositories": []
}
